sayi1=int(input("1. sayiyi giriniz :"))
sayi2=int(input("2. sayiyi giriniz :"))
if sayi1>sayi2 :
    temp=sayi1
    sayi1=sayi2
    sayi2=temp
for i in range(sayi1,0,-1):
    if (sayi1%i==0) and (sayi2%i==0) :
        ebob=i
        print("ebob=",ebob)
        break
ekok=int((sayi1*sayi2)/ebob)
print("ekok=",ekok)
